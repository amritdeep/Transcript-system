<strong>Name: </strong>
<%= @user.name %><br>	
<strong>id: </strong>
<%= @user.id %></br>
<strong>GPA: </strong>
<%= @user.gpa %><br>
<strong>Major: </strong>
<%#= @course.name %><br>
<%= label_tag 'name', @course.name, class: "course" %>
<% @seasons.each do |season| %>
<table>
	<th class="heading"><%= season.name %></th>
	<% season.subjects.each do |subject| %>
		<tr>
			<td><%= subject.name %></td>
			<td><%= subject.grade %></td>
		</tr>
	<% end %>
</table>
<% end %>
